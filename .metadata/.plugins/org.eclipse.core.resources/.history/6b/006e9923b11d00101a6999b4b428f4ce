package com.jaksim.dto;

import java.time.LocalDate;
import java.time.LocalDateTime;
import java.util.List;

import org.springframework.web.multipart.MultipartFile;

import lombok.Getter;
import lombok.Setter;

@Getter
@Setter
public class MeetingDTO {
	private Integer meetingId;
	private String meetingName;
	private String meetingWriter;
	private String meetingMission;
	private List<String> meetingYoils;
	private String meetingCategory;
	private String meetingContents;
	
	// Cover Image는 file로 전달받는다. type이 MultiPartFile 타입이다
	private MultipartFile coverImage;
	
	private LocalDateTime createdAt;
	private LocalDateTime revisedAt;
	
	//front에 전달할때 이미지 url을 전달한다.
	private String coverImageUrl;
	
	private Integer likeCnt;
	private Integer isLiked;
	
	
}
















